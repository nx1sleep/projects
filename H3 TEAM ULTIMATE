-- Scripts:

local function RLQCW_script() -- TextButton.Skybox placer 
	local script = Instance.new('LocalScript', TextButton)

	local function ChangeSkyOnClick()
		
		script.Parent.MouseButton1Click:Connect(function()
	
			local player = game.Players.LocalPlayer
			local tool
	
			for i, v in player:GetDescendants() do
				if v.Name == "SyncAPI" then
					tool = v.Parent
					break
				end
			end
	
			if not tool then
				for i, v in game.ReplicatedStorage:GetDescendants() do
					if v.Name == "SyncAPI" then
						tool = v.Parent
						break
					end
				end
			end
	
			if tool and tool:FindFirstChild("SyncAPI") then
				local remote = tool.SyncAPI.ServerEndpoint
	
				local function InvokeF3X(args)
					remote:InvokeServer(unpack(args))
				end
	
				local skyId = "117516137"
	
				local char = player.Character
				if char and char:FindFirstChild("HumanoidRootPart") then
					local pos = char.HumanoidRootPart.Position
					local x, y, z = math.floor(pos.X), math.floor(pos.Y), math.floor(pos.Z)
	
					local createArgs = {
						[1] = "CreatePart",
						[2] = "Normal", 
						[3] = CFrame.new(x, y + 10, z),
						[4] = workspace
					}
					InvokeF3X(createArgs)
	
					for i, v in workspace:GetDescendants() do
						if v:IsA("BasePart") and 
							math.floor(v.Position.X) == x and 
							math.floor(v.Position.Z) == z and
							v.Parent == workspace then
	
							local nameArgs = {
								[1] = "SetName",
								[2] = {[1] = v},
								[3] = "CustomSky"
							}
							InvokeF3X(nameArgs)
	
							local meshArgs = {
								[1] = "CreateMeshes", 
								[2] = {[1] = {["Part"] = v}}
							}
							InvokeF3X(meshArgs)
	
							local setMeshArgs = {
								[1] = "SyncMesh",
								[2] = {
									[1] = {
										["Part"] = v,
										["MeshId"] = "rbxassetid://8006679977" 
									}
								}
							}
							InvokeF3X(setMeshArgs)
	
							local textureArgs = {
								[1] = "SyncMesh", 
								[2] = {
									[1] = {
										["Part"] = v,
										["TextureId"] = "rbxassetid://" .. skyId
									}
								}
							}
							InvokeF3X(textureArgs)
	
							local resizeArgs = {
								[1] = "SyncMesh",
								[2] = {
									[1] = {
										["Part"] = v,
										["Scale"] = Vector3.new(500, 500, 500) 
									}
								}
							}
							InvokeF3X(resizeArgs)
	
							local lockArgs = {
								[1] = "SetLocked",
								[2] = {[1] = v},
								[3] = true
							}
							InvokeF3X(lockArgs)
	
							local collisionArgs = {
								[1] = "SyncCollision",
								[2] = {
									[1] = {
										["Part"] = v,
										["CanCollide"] = false
									}
								}
							}
							InvokeF3X(collisionArgs)
	
							local transArgs = {
								[1] = "SyncMaterial", 
								[2] = {
									[1] = {
										["Part"] = v,
										["Transparency"] = 0
									}
								}
							}
							InvokeF3X(transArgs)
	
							break
						end
					end
				else
					
				end
			else
				
			end
		end)
	end
	
	ChangeSkyOnClick()
end
coroutine.wrap(RLQCW_script)()
local function YMFNYAP_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		local player = game.Players.LocalPlayer
		local tool
	
		for i,v in player:GetDescendants() do
			if v.Name == "SyncAPI" then
				tool = v.Parent
			end
		end
		for i,v in game.ReplicatedStorage:GetDescendants() do
			if v.Name == "SyncAPI" then
				tool = v.Parent
			end
		end
	
		if tool then
			remote = tool.SyncAPI.ServerEndpoint
	
			function _(args)
				remote:InvokeServer(unpack(args))
			end
	
			local partsToProcess = {}
	
			for i,v in game.workspace:GetDescendants() do
				if v:IsA("BasePart") then
					table.insert(partsToProcess, v)
				end
			end
	
			for i, part in ipairs(partsToProcess) do
				local sides = {
					Enum.NormalId.Front,
					Enum.NormalId.Back, 
					Enum.NormalId.Right,
					Enum.NormalId.Left,
					Enum.NormalId.Bottom,
					Enum.NormalId.Top
				}
	
				for j, side in ipairs(sides) do
					_({
						[1] = "CreateTextures",
						[2] = {
							[1] = {
								["Part"] = part,
								["Face"] = side,
								["TextureType"] = "Decal"
							}
						}
					})
	
					_({
						[1] = "SyncTexture",
						[2] = {
							[1] = {
								["Part"] = part,
								["Face"] = side,
								["TextureType"] = "Decal",
								["Texture"] = "rbxassetid://10666996066"
							}
						}
					})
				end
			end
		end
	end)
end
coroutine.wrap(YMFNYAP_script)()
local function JCAR_script() -- TextButton_3.F3X_MassResizer 
	local script = Instance.new('LocalScript', TextButton_3)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	local tool
	
	-- Find SyncAPI tool (same as other F3X mass scripts)
	for i, v in player:GetDescendants() do
	    if v.Name == "SyncAPI" then
	        tool = v.Parent
	    end
	end
	if not tool then
	    for i, v in game.ReplicatedStorage:GetDescendants() do
	        if v.Name == "SyncAPI" then
	            tool = v.Parent
	            break
	        end
	    end
	end
	
	-- Get SyncAPI remote endpoint
	local function getSyncAPI()
	    if tool and tool:FindFirstChild("SyncAPI") and tool.SyncAPI:FindFirstChild("ServerEndpoint") then
	        return tool.SyncAPI.ServerEndpoint
	    end
	    return nil
	end
	
	-- Helper to send SyncAPI requests
	local function sendSyncAPI(args)
	    local remote = getSyncAPI()
	    if remote then
	        remote:InvokeServer(unpack(args))
	    end
	end
	
	button.MouseButton1Click:Connect(function()
	    local Players = game:GetService("Players")
	    local playerCharacters = {}
	    for i, plr in Players:GetPlayers() do
	        if plr.Character then
	            table.insert(playerCharacters, plr.Character)
	        end
	    end
	
	    local parts = {}
	    for i, v in workspace:GetDescendants() do
	        if v:IsA("BasePart") then
	            local isInPlayerModel = false
	            for j, char in playerCharacters do
	                if v:IsDescendantOf(char) then
	                    isInPlayerModel = true
	                    break
	                end
	            end
	            if not isInPlayerModel then
	                table.insert(parts, v)
	            end
	        end
	    end
	
	    for i, part in parts do
	        -- Example: resize to random size between 5 and 50 studs per axis
	        local newSize = Vector3.new(math.random(5,50), math.random(5,50), math.random(5,50))
	        sendSyncAPI({
	            "ResizePart",
	            {
	                {
	                    ["Part"] = part,
	                    ["Size"] = newSize
	                }
	            }
	        })
	    end
	end)
	
	
end
coroutine.wrap(JCAR_script)()
local function BAKYK_script() -- TextButton_4.F3X_MassPartSpawner 
	local script = Instance.new('LocalScript', TextButton_4)

	local button = script.Parent
	
	local function findSyncTool()
	    local player = game.Players.LocalPlayer
	    local tool
	    for i, v in player:GetDescendants() do
	        if v.Name == "SyncAPI" then
	            tool = v.Parent
	            break
	        end
	    end
	    if not tool then
	        for i, v in game.ReplicatedStorage:GetDescendants() do
	            if v.Name == "SyncAPI" then
	                tool = v.Parent
	                break
	            end
	        end
	    end
	    return tool
	end
	
	local function getSyncRemote(tool)
	    if tool and tool:FindFirstChild("SyncAPI") and tool.SyncAPI:FindFirstChild("ServerEndpoint") then
	        return tool.SyncAPI.ServerEndpoint
	    end
	    return nil
	end
	
	local function InvokeF3X(args, remote)
	    if remote then
	        remote:InvokeServer(unpack(args))
	    end
	end
	
	button.MouseButton1Click:Connect(function()
	    local tool = findSyncTool()
	    local remote = getSyncRemote(tool)
	    if not remote then return end
	
	    local Players = game:GetService("Players")
	    for i, plr in Players:GetPlayers() do
	        if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
	            local pos = plr.Character.HumanoidRootPart.Position
	            local x, y, z = math.floor(pos.X), math.floor(pos.Y), math.floor(pos.Z)
	            for j = 1, 5 do
	                local offset = 5 + (j-1)*2
	                local spawnCFrame = CFrame.new(x, y + offset, z)
	                local createArgs = {
	                    [1] = "CreatePart",
	                    [2] = "Normal",
	                    [3] = spawnCFrame,
	                    [4] = workspace
	                }
	                InvokeF3X(createArgs, remote)
	
	                -- Find the newly created part by position and parent
	                local newPart = nil
	                for k, v in workspace:GetDescendants() do
	                    if v:IsA("BasePart") and v.Parent == workspace then
	                        if (v.Position - spawnCFrame.Position).Magnitude < 1 then
	                            newPart = v
	                            break
	                        end
	                    end
	                end
	
	                if newPart then
	                    local anchorArgs = {
	                        [1] = "SyncProperties",
	                        [2] = {
	                            [1] = {
	                                ["Part"] = newPart,
	                                ["Anchored"] = false
	                            }
	                        }
	                    }
	                    InvokeF3X(anchorArgs, remote)
	                end
	            end
	        end
	    end
	end)
	
	
end
coroutine.wrap(BAKYK_script)()
local function MVFGYND_script() -- TextButton_5.F3X_MassColorChanger 
	local script = Instance.new('LocalScript', TextButton_5)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	local tool
	
	for i, v in player:GetDescendants() do
	    if v.Name == "SyncAPI" then
	        tool = v.Parent
	    end
	end
	for i, v in game.ReplicatedStorage:GetDescendants() do
	    if v.Name == "SyncAPI" then
	        tool = v.Parent
	    end
	end
	
	local function getSyncAPI()
	    if tool and tool:FindFirstChild("SyncAPI") and tool.SyncAPI:FindFirstChild("ServerEndpoint") then
	        return tool.SyncAPI.ServerEndpoint
	    end
	    return nil
	end
	
	local function sendSyncAPI(args)
	    local remote = getSyncAPI()
	    if remote then
	        remote:InvokeServer(unpack(args))
	    end
	end
	
	button.MouseButton1Click:Connect(function()
	    local parts = {}
	    for i, v in game.Workspace:GetDescendants() do
	        if v:IsA("BasePart") then
	            table.insert(parts, v)
	        end
	    end
	    for i, part in parts do
	        sendSyncAPI({
	            "SyncColor",
	            {
	                {
	                    ["Part"] = part,
	                    ["Color"] = Color3.fromRGB(math.random(0,255), math.random(0,255), math.random(0,255))
	                }
	            }
	        })
	    end
	end)
	
	
end
coroutine.wrap(MVFGYND_script)()
