local function DVAV_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	local button = script.Parent
	local RequestCommand = game:GetService("ReplicatedStorage"):WaitForChild("HDAdminHDClient").Signals.RequestCommandSilent
	local Players = game:GetService("Players")
	
	local isActive = false
	local spamCoroutine = nil
	
	local function startTitleSpam()
		while isActive do
			local playersList = Players:GetPlayers()
	
			for _, player in ipairs(playersList) do
				if not isActive then break end
				if player ~= Players.LocalPlayer then
					RequestCommand:InvokeServer(";titleg " .. player.Name .. " h3 team!")
					wait(0.3)
				end
			end
	
			if isActive then
				wait(1)
			end
		end
	end
	
	button.MouseButton1Click:Connect(function()
		isActive = not isActive
	
		if isActive then
			button.Text = "Title all ON"
			spamCoroutine = coroutine.create(startTitleSpam)
			coroutine.resume(spamCoroutine)
		else
			button.Text = "Title all OFF"
		end
	end)
end
coroutine.wrap(DVAV_script)()
local function CGOXW_script() -- TextButton_7.LocalScript 
	local script = Instance.new('LocalScript', TextButton_7)

	local button = script.Parent
	local RequestCommand = game:GetService("ReplicatedStorage"):WaitForChild("HDAdminHDClient").Signals.RequestCommandSilent
	local Players = game:GetService("Players")
	
	local isActive = false
	local spamCoroutine = nil
	
	local function startFireSparkleSpam()
		while isActive do
			local playersList = Players:GetPlayers()
	
			for _, player in ipairs(playersList) do
				if not isActive then break end
				if player ~= Players.LocalPlayer then -- Пропускаем себя
					RequestCommand:InvokeServer(";fire " .. player.Name)
					wait(0.2)
				end
			end
	
			if isActive then
				wait(0.5)
			end
	
			for _, player in ipairs(playersList) do
				if not isActive then break end
				if player ~= Players.LocalPlayer then
					RequestCommand:InvokeServer(";sparkles " .. player.Name)
					wait(0.2)
				end
			end
	
			if isActive then
				wait(1)
			end
		end
	end
	
	button.MouseButton1Click:Connect(function()
		isActive = not isActive
	
		if isActive then
			button.Text = "Fire n Sparkle ON"
			spamCoroutine = coroutine.create(startFireSparkleSpam)
			coroutine.resume(spamCoroutine)
		else
			button.Text = "Fire n Sparkle OFF"
		end
	end)
end
coroutine.wrap(CGOXW_script)()
